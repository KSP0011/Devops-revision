echo "This change is in the dev-feature branch" >> file1.txt
# âœ… Git Practice Summary (Day 1)

# ðŸ”¹ Initialize Git Repository
git init

# ðŸ”¹ Check Git Status
git status

# ðŸ”¹ Create and Commit a File
echo "hello git" > file1.txt
git add file1.txt
git commit -m "Initial commit with file1.txt"

# ðŸ”¹ View Commit Log
git log

# ðŸ”¹ View Unstaged Changes (after editing)
git diff

# ðŸ”€ Branching
# Create and switch to a new branch
git branch dev-feature
git checkout dev-feature

# Make a change in the new branch
echo "This change is in the dev-feature branch" >> file1.txt
git diff
git add file1.txt
git commit -m "Added branch-specific line in dev-feature"

# Switch back to main branch
git checkout master

# Verify file content (change not visible here)
cat file1.txt
